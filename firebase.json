{
  "hosting": {
    "public": "public",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "cleanUrls": true,
    "trailingSlash": false,
    "headers": [
      {
        "source": "**/*.css",
        "headers": [{"key": "Cache-Control", "value": "public, max-age=31536000"}]
      },
      {
        "source": "**/*.html",
        "headers": [{"key": "Cache-Control", "value": "public, max-age=3600"}]
      }
    ]
  }
}
